      "source": [
        "# Atribuindo os valores\n",
        "Y será a classe (se é comestível ou venenoso)\n",
        "X serão todos os outros atributos\n",
        "y tera os valores 0 e 1 para representar edible ou poisoneus respectivamente"
      ],
      "metadata": {
        "id": "8JVVBqfq1VQh"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "df['class'] = df['class'].replace({'e': 0, 'p': 1})\n",
        "y = df['class']\n",
        "X = df.drop('class',axis = 1)\n",
        "y\n"
      ],
