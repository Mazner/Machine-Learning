      "cell_type": "code",
      "source": [
        "from sklearn.compose import ColumnTransformer\n",
        "\n",
        "X.reset_index(drop=True, inplace=True)\n",
        "\n",
        "transformers = []\n",
        "for coluna in X.columns:\n",
        "    transformers.append((coluna, OneHotEncoder(), [coluna]))\n",
        "\n",
        "ct_oh = ColumnTransformer(transformers, remainder='passthrough')\n",
        "X_oh = ct_oh.fit_transform(X).todense()"
      ],
      "metadata": {
        "id": "u80J51EWGunP",
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "outputId": "862db8f0-0a38-4351-8771-ee9fd14ebeaa"
      },
